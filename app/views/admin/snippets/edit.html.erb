<% @page_title = "Editing snippet" %>

<% form_for([:admin, @site, @snippet]) do |f| %>
  <%= render :partial => "form", :locals => { :f => f } %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to "Show", [:admin, @site, @snippet] %> |
<%= link_to "Back", admin_site_snippets_path(@site) %>
